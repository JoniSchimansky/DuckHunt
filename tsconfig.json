  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "outDir": "./app/dist",
    "types": ["vite/client"],
  }
}
